{
  "name": "Agent Align",
  "image": "mcr.microsoft.com/devcontainers/go:1.22",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "20"
    }
  },
  "workspaceFolder": "/workspaces/agent-align",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/agent-align,type=bind,consistency=cached",
  "postCreateCommand": "npm ci",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "forwardPorts": [],
  "remoteUser": "vscode"
}
